{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "version": 2,
    "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9"
      }
    }
  ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
  }
  